{
    "name": "wlwwt/sw-composer-installers",
    "type": "composer-plugin",
    "license": "GPL-2.0+",
    "description": "Shopware Installers",
    "keywords": [
        "installer", "shopware", "core", "plugin"
    ],
    "homepage": "https://github.com/wlwwt/SwComposerInstallers",
    "authors": [
        {
            "name": "Wolfgang Freund",
            "email": "wolfgang.freund@wlwwt.de"
        }
    ],
    "autoload": {
        "psr-4": {
            "Wlwwt\\Sw\\Composer\\": "src/"
        }
    },
    "require": {
        "php": ">=5.5.0",
        "composer-plugin-api": "^1.0.0"
    },
    "conflict": {
        "composer/installers": "<1.0.24 || >1.0.24"
    },
    "require-dev": {
        "composer/composer": "^1.2"
    },
    "extra": {
        "class": "Wlwwt\\Sw\\Composer\\Installer\\Plugin",
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    }
}
